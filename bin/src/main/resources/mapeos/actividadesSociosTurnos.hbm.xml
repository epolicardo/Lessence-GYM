<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 
<hibernate-mapping>

    <class name="com.lessence.gymmaven.clases.Actividades_Socios_Turnos" table="ACTIVIDADES_SOCIOS_TURNOS">
        <id name="idRelacion" column="idRelacion">
            <generator class="identity"/>
        </id>
        <property name="pases" column="pases" type="int"/>
        <many-to-one name="socio" cascade="all"/>
        <many-to-one name="relacion" cascade="all"/>
        
        
    </class>
  

</hibernate-mapping>
