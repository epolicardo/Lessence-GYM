<!DOCTYPE hibernate-mapping PUBLIC  "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>

    <class name="com.lessence.gymmaven.clases.Gastos" table="GASTOS">
        <id name="idGasto" column="idGasto">
            <generator class="identity"/>
        </id>
        <property name="fecha" column="fecha" type="timestamp" />
        <property name="concepto" column="concepto" type="string" />
        <property name="importe" column="importe" type="float" />
        <property name="tipo" column="tipo" type="int" />

    </class>
</hibernate-mapping>
